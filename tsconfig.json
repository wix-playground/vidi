{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "target": "es5",
        "noImplicitAny": false,
        "strictNullChecks": true,
        "sourceMap": true,
        "inlineSources": true,
        "outDir": "dist",
        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "exclude": [
        "node_modules",
        "http-media-server/assets",
        "dist",
        "demo"
    ]
}